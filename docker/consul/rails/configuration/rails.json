{
  "service": {
    "name": "rails-server",
    "tags": ["app"],
    "address": "EXTERNAL_IP",
    "port": EXTERNAL_PORT,
    "id": "SERVICE_ID",
    "checks": [
      {
        "id": "rails-app-server-check",
        "name": "Rails check",
        "script": "/usr/local/bin/rails-app-server-check.sh",
        "interval": "30s"
      }
    ]
  }
}
