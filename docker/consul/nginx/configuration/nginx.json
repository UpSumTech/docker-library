{
  "service": {
    "name": "nginx-server",
    "tags": ["web"],
    "address": "EXTERNAL_IP",
    "port": EXTERNAL_PORT,
    "id": "SERVICE_ID",
    "checks": [
      {
        "id": "nginx-server-check",
        "name": "Nginx check",
        "script": "/usr/local/bin/nginx-server-check.sh",
        "interval": "30s"
      }
    ]
  }
}
